    <servlet-class>com.servlet.GodisnjeDoba</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ljeto</servlet-name>
    <servlet-class>com.servlet.Ljeto</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>zima</servlet-name>
    <servlet-class>com.servlet.Zima</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>proljece</servlet-name>
    <servlet-class>com.servlet.Proljece</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>jesen</servlet-name>
    <servlet-class>com.servlet.Jesen</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>filter</servlet-name>
    <servlet-class>com.servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>godisnjedoba</servlet-name>
    <url-pattern>/krenimo.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ljeto</servlet-name>
    <url-pattern>/ljeto.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>zima</servlet-name>
    <url-pattern>/zima.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>proljece</servlet-name>
    <url-pattern>/proljece.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>jesen</servlet-name>
    <url-pattern>/jesen.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>filter</servlet-name>
    <url-pattern>/login.html</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>